{
  "name": "chatfoundry",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:prd": "cross-env NODE_ENV=production react-router build",
    "dev": "react-router dev",
    "preview": "pnpm run build && vite preview",
    "wrangler:deploy": "cross-env NODE_ENV=production wrangler deploy",
    "deploy": "run-s db:apply-prod build:prd wrangler:deploy",
    "deploy:version": "react-router build && wrangler versions upload",
    "deploy:promote": "wrangler versions deploy",
    "start": "wrangler dev --test-scheduled",
    "typecheck": "tsc",
    "types:wrangler": "wrangler types",
    "types:react-router": "react-router typegen",
    "types:generate": "run-s types:wrangler types:react-router",
    "check": "biome check .",
    "check:fix": "biome check --fix .",
    "format": "biome format --write .",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:apply": "wrangler d1 migrations apply CF_DB --local",
    "db:apply-prod": "wrangler d1 migrations apply CF_DB --remote"
  },
